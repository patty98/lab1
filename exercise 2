#include<stdio.h>
#include <stdlib.h>
int main(){
	int a[9]={1,2,3,4,5,5,5,6,34};
	int i,k,n,b;
	n=0;
	for (i=0;i<9;i++){
		if (a[i]==a[i+1]){
			a[i]=0;
		}
		else {
			continue;}
	}
	
for (k=0;k<9;k++){
	if (a[k]==0){
		a[k]=a[k+1];
	}
	else {
		continue;}
}
	

printarray(a);
}

int printarray(const int a[]){
	int s;
for (s=0;s<9;s++){
	printf("%d",a[s]);
}


}
